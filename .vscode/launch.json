    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "name": "Launch interface of Feed generator",
            "request": "launch",
            "program": "${workspaceFolder}/src/index.ts --operation feed",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "name": "Launch indexer of Feed generator",
            "request": "launch",
            "program": "${workspaceFolder}/src/index.ts --operation index",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}